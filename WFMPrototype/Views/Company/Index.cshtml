
@{
    ViewBag.Title = "Manage Company";
    Layout = "~/Views/Shared/_NewLayout.cshtml";
}

 <!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Manage Company

    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>

        <li class="active">Company</li>
    </ol>
</section>
<!-- Main content -->

<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="nav-tabs-custom">
                <ul class="nav nav-tabs">
                    <li class="active"><a href="#fa-icons" data-toggle="tab">Create Company</a></li>
                    <li><a href="#glyphicons" data-toggle="tab">Company List</a></li>
                </ul>
                <div class="tab-content">
                    <!-- Font Awesome Icons -->
                    <div class="tab-pane active" id="fa-icons">

                        <!-- Horizontal Form -->
                        <!-- /.box-header -->
                        <!-- form start -->
                        <form class="form-horizontal" id="companyform">
                            <div class="box-body">
                                <div class="form-group">
                                    <label for="txtcompanyname" class="col-sm-2 control-label">Company Name</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" name="txtcompanyname" id="txtcompanyname" placeholder="Company Name">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtcompanyaddress" class="col-sm-2 control-label">Company Address</label>
                                    <div class="col-sm-10">
                                        <textarea type="text" class="form-control" id="txtcompanyaddress" name="txtcompanyaddress" placeholder="Company Address"></textarea>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtcompanyphone" class="col-sm-2 control-label">Company Phone</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" id="txtcompanyphone" name="txtcompanyphone" placeholder="Company Phone">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtownername" class="col-sm-2 control-label">Owner Name</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" id="txtownername" name="txtownername" placeholder="Owner Name">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtusername" class="col-sm-2 control-label">Email/User Name</label>
                                    <div class="col-sm-10">
                                        <input type="email" class="form-control" id="txtusername" name="txtusername" placeholder="Email/User Name">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtpassword" class="col-sm-2 control-label">Password</label>
                                    <div class="col-sm-10">
                                        <input type="password" class="form-control" id="txtpassword" name="txtpassword" placeholder="Password">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtownermobile" class="col-sm-2 control-label">Owner Mobile</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" id="txtownermobile" name="txtownermobile" placeholder="Owner Mobile">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="ddlstate" class="col-sm-2 control-label">State</label>
                                    <div class="col-sm-10"  >
                                        <select class="form-control" id="ddlstate" name="ddlstate">
                                            <option value="">Select State</option>
                                            <option value="Rajasthan">Rajasthan</option>
                                            <option value="Gujrat">Gujrat</option>
                                            <option value="MP">MP</option>
                                            <option value="UP">UP</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="ddlcity" class="col-sm-2 control-label">City</label>
                                    <div class="col-sm-10">
                                        <select class="form-control" id="ddlcity" name="ddlcity">
                                            <option value="">Select State</option>
                                            <option value="Udaipur">Udaipur</option>
                                            <option value="Jaipur">Jaipur</option>
                                            <option value="Bhilwara">Bhilwara</option>
                                            <option value="Jodhpur">Jodhpur</option>
                                        </select>
                                    </div>
                                </div>

                            </div>
                            <!-- /.box-body -->
                            <div class="box-footer">
                                <button type="submit" class="btn btn-info pull-right" style="margin-left:10px">Submit</button>
                                
                                <button type="submit" class="btn btn-info pull-right">Cancel</button>

                            </div>
                            <!-- /.box-footer -->
                        </form>
                        <!-- /.box -->


                    </div>
                    <!-- /#fa-icons -->
                    <!-- glyphicons-->
                    <div class="tab-pane" id="glyphicons">
                        <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th>Company Name</th>
                                    <th>Company Address</th>
                                    <th>Company Phone</th>
                                    <th>Owner Name</th>
                                    <th>Email/User Name</th>
                                    <th>Owner Mobile</th>
                                    <th>State</th>
                                    <th>City</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>Tiger Nixon</td>
                                    <td>System Architect</td>
                                    <td>Edinburgh</td>
                                    <td>Suraj</td>
                                    <td>company@gmail.com</td>
                                    <td>9824319758</td>
                                    <td>Rajasthan</td>
                                    <td>Bhilwara</td>
                                </tr>
                                <tr>
                                    <td>Tiger Nixon</td>
                                    <td>System Architect</td>
                                    <td>Edinburgh</td>
                                    <td>Suraj</td>
                                    <td>company@gmail.com</td>
                                    <td>9824319758</td>
                                    <td>Rajasthan</td>
                                    <td>Bhilwara</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <!-- /#ion-icons -->
                </div>
                <!-- /.tab-content -->
            </div>
            <!-- /.nav-tabs-custom -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>

<script src="~/JS/Company/ManageCompany.js"></script>