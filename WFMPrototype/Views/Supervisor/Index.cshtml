
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_NewLayout.cshtml";
}

<section class="content-header">
    <h1>
        Manage Supervisor

    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>

        <li class="active">Manage Supervisor</li>
    </ol>
</section>
<!-- Main content -->

<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="nav-tabs-custom">
                <ul class="nav nav-tabs">
                    <li class="active"><a id="AddSupervisor" href="#fa-icons" data-toggle="tab"><span id="spantext"></span></a></li>
                    <li><a href="#glyphicons" data-toggle="tab">Supervisor List</a></li>
                </ul>
                <div class="tab-content">
                    <div class="tab-pane active" id="fa-icons">

                        <!-- Horizontal Form -->
                        <!-- /.box-header -->
                        <!-- form start -->
                        <form class="form-horizontal" id="supervisorform">
                            <div class="box-body">
                                <input type="hidden" id="hdnsupervisor" value="0" />
                                <input type="hidden" id="hdnsupervisorID" value="0" />
                                <div class="form-group">
                                    <label for="txtfirstname" class="col-sm-2 control-label">First Name</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" id="txtfirstname" name="txtfirstname" placeholder="First Name">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtmiddlename" class="col-sm-2 control-label">Middle Name</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" id="txtmiddlename" name="txtmiddlename" placeholder="Middle Name">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtlastname" class="col-sm-2 control-label">Last Name</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" id="txtlastname" name="txtlastname" placeholder="Last Name">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtfathername" class="col-sm-2 control-label">Father Name</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" id="txtfathername" name="txtfathername" placeholder="Father Name">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtemail" class="col-sm-2 control-label">Email/User Name</label>
                                    <div class="col-sm-10">
                                        <input type="email" class="form-control" id="txtemail" name="txtemail" placeholder="Email/User Name">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtpassword" class="col-sm-2 control-label">Password</label>
                                    <div class="col-sm-10">
                                        <input type="password" class="form-control" id="txtpassword" name="txtpassword" placeholder="Password">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtmobile" class="col-sm-2 control-label">Mobile</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" id="txtmobile" name="txtmobile" placeholder="Mobile">
                                    </div>
                                </div>
                              
                                <div class="form-group">
                                    <label for="ddlgender" class="col-sm-2 control-label">Gender</label>
                                    <div class="col-sm-10">
                                        <select class="form-control" id="ddlgender" name="ddlgender">
                                            <option value="">Select Gender</option>
                                            <option value="Male">Male</option>
                                            <option value="Female">Female</option>

                                        </select>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="ddlstate" class="col-sm-2 control-label">State</label>
                                    <div class="col-sm-10">
                                        <select class="form-control" id="ddlstate" name="ddlstate">
                                            <option value="">Select State</option>
                                            <option value="Rajasthan">Rajasthan</option>
                                            <option value="Gujrat">Gujrat</option>
                                            <option value="MP">MP</option>
                                            <option value="UP">UP</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="ddlcity" class="col-sm-2 control-label">City</label>
                                    <div class="col-sm-10">
                                        <select class="form-control" id="ddlcity" name="ddlcity">
                                            <option value="">Select City</option>
                                            <option value="Udaipur">Udaipur</option>
                                            <option value="Jaipur">Jaipur</option>
                                            <option value="Bhilwara">Bhilwara</option>
                                            <option value="Jodhpur">Jodhpur</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtparmanentaddress" class="col-sm-2 control-label">Parmanent Address</label>
                                    <div class="col-sm-10">
                                        <textarea type="text" class="form-control" id="txtparmanentaddress" name="txtparmanentaddress" placeholder="Parmanent Address"></textarea>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtcurrentaddress" class="col-sm-2 control-label">Current Address</label>
                                    <div class="col-sm-10">
                                        <textarea type="text" class="form-control" id="txtcurrentaddress" name="txtcurrentaddress" placeholder="Current Address"></textarea>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="fuidproof" class="col-sm-2 control-label">ID Proof</label>
                                    <div class="col-sm-10">
                                        <input type="file" id="fuidproof" name="fuidproof">
                                    </div>
                                </div>

                            </div>
                            <!-- /.box-body -->
                            <div class="box-footer">
                                <button type="submit" class="btn btn-info pull-right" style="margin-left:10px">Submit</button>

                                <button type="button" onclick="Cancel()" class="btn btn-info pull-right">Cancel</button>

                            </div>
                            <!-- /.box-footer -->
                        </form>
                        <!-- /.box -->


                    </div>
                    <!-- /#fa-icons -->
                    <!-- glyphicons-->
                    <div class="table-responsive tab-pane" id="glyphicons">
                        <table id="tblsupervisor" class="table table-striped table-bordered" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                   
                                    <th>First Name</th>                                   
                                    <th>Middle Name</th>
                                    <th>Last Name</th>
                                    <th>Father Name</th>
                                    <th>Email/User Name</th>
                                    <th>Password</th> 
                                    <th>Mobile</th>                                  
                                    <th>Gender</th>
                                    <th>State</th>
                                    <th>City</th>
                                    <th>Parmanent Address</th>
                                    <th>Current Address</th>
                                    <th>ID Proof</th>
                                    <th></th>
                                    <th></th>
                                </tr>
                            </thead>

                        </table>
                    </div>
                </div>
            </div>
            <!-- /.nav-tabs-custom -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>

<script src="~/JS/Supervisor/supervisor.js"></script>