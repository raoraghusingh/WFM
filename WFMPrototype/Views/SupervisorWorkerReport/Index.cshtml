
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_NewLayout.cshtml";
}

<section class="content-header">
    <h1>
        Supervisor & Worker Report

    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>

        <li class="active">Supervisor & Worker Report</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="nav-tabs-custom">
                <ul class="nav nav-tabs">

                    <li class="active"><a href="#glyphicons" data-toggle="tab">Supervisor & Worker Report</a></li>
                </ul>
                <div class="tab-content">
                    <!-- Font Awesome Icons -->
                    <!-- /#fa-icons -->
                    <!-- glyphicons-->
                    <div class="tab-pane active" id="glyphicons">
                        <form id="supervisorworkerreportform" class="form-horizontal">                        
                            

                            <div class="form-group">

                                <label for="inputEmail3" id="" name="" class="col-sm-2 control-label">Company Name</label>
                                <div class="col-sm-10">
                                    <select id="ddlcompanyname" name="ddlcompanyname" class="form-control"></select>
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="ddlusertype" class="col-sm-2 control-label">User Type</label>
                                <div class="col-sm-10">
                                    <select class="form-control" id="ddlusertype" name="ddlusertype">
                                        <option value="">Select User Type</option>
                                        <option value="1">Supervisor</option>
                                        <option value="2">Worker</option>

                                    </select>
                                </div>
                            </div><br/><br/>
                            <div class="box-footer">
                                <button type="submit" class="btn btn-info pull-right" style="margin-left:10px">Submit</button>
                                <button type="button" onclick="Cancel()" class="btn btn-info pull-right">Cancel</button>

                            </div>
                        </form>
                        <br /><br />
                        <div class="table-responsive tab-pane" id="glyphicons">
                            <table id="tblsupervisorworkerreport" class="table table-striped table-bordered" cellspacing="0" width="100%">
                                <thead>
                                    <tr>
                                        <th>First Name</th>
                                        <th>Middle Name</th>
                                        <th>Last Name</th>
                                        <th>Father Name</th>
                                        <th>Email ID</th>
                                        <th>Mobile</th>
                                        <th>Gender</th>
                                        <th>State</th>
                                        <th>City</th>
                                        <th>Parmanent Address</th>
                                        <th>Current Address</th>
                                        <th>ID Proof</th>
                                    </tr>
                                </thead>

                            </table>
                        </div>
                    <!-- /#ion-icons -->
                </div>
                <!-- /.tab-content -->
            </div>
            <!-- /.nav-tabs-custom -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>
<script src="~/JS/SupervisorWorkerReport/SupervisorWorkerReport.js"></script>
