
@{
    ViewBag.Title = "Ticket history";
    Layout = "~/Views/Shared/_NewLayout.cshtml";
}

<section class="content-header">
    <h1>
        Manage Ticket

    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>

        <li class="active">Ticket</li>
    </ol>
</section>
<!-- Main content -->

<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="nav-tabs-custom">
                <ul class="nav nav-tabs">
                    <li class="active"><a href="#fa-icons" id="addcompany" data-toggle="tab">Active & Pending Ticket</a></li>
                    <li><a href="#glyphicons" data-toggle="tab">Resolved Ticket</a></li>
                </ul>
                <div class="tab-content">
                    <!-- Font Awesome Icons -->
                    <div class="tab-pane active" id="fa-icons">

                        <table id="tblpendingandactiveatable" class="table table-striped table-bordered" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th>Ticket Number</th>
                                    <th>Company Name</th>
                                    <th>Complain By</th>
                                    <th>Mobile</th>
                                    <th>Email ID</th>
                                    <th>Assign Worker</th>
                                    <th>Status</th>
                                    <th></th>
                                </tr>
                            </thead>

                        </table>


                    </div>
                    <!-- /#fa-icons -->
                    <!-- glyphicons-->
                    <div class="table-responsive tab-pane" id="glyphicons">
                        <table id="tblresolvedticket" class="table table-striped table-bordered" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th>Ticket Number</th>
                                    <th>Company Name</th>
                                    <th>Complain By</th>
                                    <th>Mobile</th>
                                    <th>Email ID</th>
                                    <th>Assign Worker</th>
                                    <th>Status</th>
                                    
                                </tr>
                            </thead>

                        </table>
                    </div>
                    <!-- /#ion-icons -->
                </div>
                <!-- /.tab-content -->
            </div>
            <!-- /.nav-tabs-custom -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>
<script src="~/JS/Ticket/Ticket.js"></script>
