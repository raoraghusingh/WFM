
@{
    ViewBag.Title = "Organization Setup";
    Layout = "~/Views/Shared/_SuperAdminLayout.cshtml";
}

 <!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Manage Organization

    </h1>
    <ol class="breadcrumb">
        <li><a href="/Super-Admin-Dashboard"><i class="fa fa-dashboard"></i> Home</a></li>

        <li class="active">Organization</li>
    </ol>
</section>
<!-- Main content -->

<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="nav-tabs-custom">
                <ul class="nav nav-tabs">
                    <li class="active"><a href="#fa-icons" id="addorganization" data-toggle="tab"><span id="spantext"></span> </a></li>
                    <li><a href="#glyphicons" data-toggle="tab">Organization List</a></li>
                </ul>
                <div class="tab-content">
                    <!-- Font Awesome Icons -->
                    <div class="tab-pane active" id="fa-icons">

                        <!-- Horizontal Form -->
                        <!-- /.box-header -->
                        <!-- form start -->
                        <form class="form-horizontal" id="Organizationform">
                            <div class="box-body">
                                <div class="form-group">
                                    <input type="hidden" id="hdnorganization" value="0" />
                                    <label for="txtcompanyname" class="col-sm-2 control-label">Organization Name</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" name="txtcompanyname" id="txtcompanyname" placeholder="Organization Name">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtfirstname" class="col-sm-2 control-label">First Name</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" id="txtfirstname" name="txtfirstname" placeholder="First Name">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtmiddle" class="col-sm-2 control-label">Middle Name</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" id="txtmiddle" name="txtmiddle" placeholder="Middle Name">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtlastname" class="col-sm-2 control-label">Last Name</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" id="txtlastname" name="txtlastname" placeholder="Last Name">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtusername" class="col-sm-2 control-label">Email/User Name</label>
                                    <div class="col-sm-10">
                                        <input type="email" class="form-control" id="txtusername" name="txtusername" placeholder="Email/User Name">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtpassword" class="col-sm-2 control-label">Password</label>
                                    <div class="col-sm-10">
                                        <input type="password" class="form-control" id="txtpassword" name="txtpassword" placeholder="Password">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtownermobile" class="col-sm-2 control-label">Mobile</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" id="txtownermobile" name="txtownermobile" placeholder="Mobile">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtDesignation" class="col-sm-2 control-label">Designation</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" id="txtDesignation" name="txtDesignation" placeholder="Designation">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="ddlGender" class="col-sm-2 control-label">Gender</label>
                                    <div class="col-sm-10">
                                        <select class="form-control" id="ddlGender" name="ddlGender">
                                            <option value="">Select Gender</option>
                                            <option value="Male">Male</option>
                                            <option value="Female">Female</option>
                                            
                                        </select>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label for="ddlstate" class="col-sm-2 control-label">State</label>
                                    <div class="col-sm-10">
                                        <select class="form-control" id="ddlstate" name="ddlstate">
                                            <option value="">Select State</option>
                                            <option value="Rajasthan">Rajasthan</option>
                                            <option value="Gujrat">Gujrat</option>
                                            <option value="MP">MP</option>
                                            <option value="UP">UP</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="ddlcity" class="col-sm-2 control-label">City</label>
                                    <div class="col-sm-10">
                                        <select class="form-control" id="ddlcity" name="ddlcity">
                                            <option value="">Select State</option>
                                            <option value="Udaipur">Udaipur</option>
                                            <option value="Jaipur">Jaipur</option>
                                            <option value="Bhilwara">Bhilwara</option>
                                            <option value="Jodhpur">Jodhpur</option>
                                        </select>
                                    </div>
                                </div>

                            </div>
                            <!-- /.box-body -->
                            <div class="box-footer">
                                <button type="submit" class="btn btn-info pull-right" style="margin-left:10px">Submit</button>

                                <button type="button" onclick="Cancel()" class="btn btn-info pull-right">Cancel</button>

                            </div>
                            <!-- /.box-footer -->
                        </form>
                        <!-- /.box -->


                    </div>
                    <!-- /#fa-icons -->
                    <!-- glyphicons-->
                    <div class="tab-pane" id="glyphicons">
                        <table id="tblorganization" class="table table-striped table-bordered" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th>Organization Name</th>
                                    <th>Name</th>
                                    <th>User Name</th>
                                    <th>Mobile Number</th>
                                    <th>Designation</th>
                                    <th>State</th>
                                    <th>City</th>
                                    <th></th>
                                    <th></th>
                                </tr>
                            </thead>
                            
                        </table>
                    </div>
                    <!-- /#ion-icons -->
                </div>
                <!-- /.tab-content -->
            </div>
            <!-- /.nav-tabs-custom -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>

<script src="~/JS/Organization/Organization.js"></script>