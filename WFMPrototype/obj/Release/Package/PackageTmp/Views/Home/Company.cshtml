
@{
    ViewBag.Title = "Company";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

 <!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Manage Company
        
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
       
        <li class="active">General Elements</li>
    </ol>
</section>
